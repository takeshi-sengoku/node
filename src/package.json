{
  "name": "sample-rest-api",
  "version": "1.0.0",
  "description": "`Swagger`を使用したREST APIのサンプルです ",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run dellocalpkg",
    "start": "NODE_ENV=production node index.js",
    "debug": "NODE_ENV=development DEBUG=* node --inspect=node.local-fw.com --debug-brk index.js",
    "dev": "NODE_ENV=development DEBUG=* node node_modules/supervisor/lib/cli-wrapper.js -i package.json,node_modules -e node,js,json ./index.js",
    "coverage": "node  node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha",
    "dellocalpkg": "node delete-local-package.js",
    "postdellocalpkg": "npm install --no-bin-links"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "config": "1.24.0",
    "connect": "3.5.0",
    "cookie-parser": "1.4.3",
    "debug": "2.6.0",
    "express": "4.14.0",
    "helmet": "3.4.0",
    "js-yaml": "3.7.0",
    "log4js": "1.1.0",
    "moment": "2.17.1",
    "morgan": "1.7.0",
    "mysql2": "1.1.2",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "redis": "2.6.4",
    "request": "2.79.0",
    "sample-connection-pool-middleware": "file:vendor/sample-connection-pool-middleware",
    "sample-db": "file:vendor/sample-db",
    "sample-http-access-logger": "file:vendor/sample-http-access-logger",
    "sample-logger": "file:vendor/sample-logger",
    "sample-performance-logger": "file:vendor/sample-performance-logger",
    "sample-request-logger": "file:vendor/sample-request-logger",
    "serve-favicon": "2.3.2",
    "socket.io": "1.7.2",
    "sprintf-js": "1.0.3",
    "supervisor": "0.12.0",
    "swagger-tools": "0.10.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "istanbul": "0.4.5",
    "jshint": "2.9.4",
    "mocha": "3.2.0",
    "supertest": "3.0.0"
  }
}
